<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>adminlogin</servlet-name>
        <servlet-class>.adminlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admlogin</servlet-name>
        <servlet-class>com.admin.admlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>usrlogin</servlet-name>
        <servlet-class>com.users.usrlogin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>userReg</servlet-name>
        <servlet-class>com.users.userReg</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addproduct</servlet-name>
        <servlet-class>com.admin.Addproduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Addemployee</servlet-name>
        <servlet-class>com.admin.Addemployee</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admlogout</servlet-name>
        <servlet-class>com.admin.admlogout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteemp</servlet-name>
        <servlet-class>com.admin.deleteemp</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>deleteprd</servlet-name>
        <servlet-class>com.admin.deleteprd</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adminlogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admlogin</servlet-name>
        <url-pattern>/admlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>usrlogin</servlet-name>
        <url-pattern>/usrlogin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>userReg</servlet-name>
        <url-pattern>/userReg</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addproduct</servlet-name>
        <url-pattern>/Addproduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Addemployee</servlet-name>
        <url-pattern>/Addemployee</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admlogout</servlet-name>
        <url-pattern>/admlogout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteemp</servlet-name>
        <url-pattern>/deleteemp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>deleteprd</servlet-name>
        <url-pattern>/deleteprd</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
